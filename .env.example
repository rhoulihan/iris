# IRIS Development Environment Configuration
# Copy this file to .env and update with your actual values

# Environment
ENVIRONMENT=development

# Oracle Database
ORACLE_HOST=localhost
ORACLE_PORT=1524
ORACLE_SERVICE=FREEPDB1
ORACLE_USER=iris_user
ORACLE_PASSWORD=your_password_here
ORACLE_SYS_PASSWORD=your_sys_password_here

# MinIO (S3-Compatible Storage)
MINIO_ENDPOINT=http://localhost:9000
MINIO_ACCESS_KEY=iris-admin
MINIO_SECRET_KEY=your_minio_secret_here
MINIO_BUCKET=mlflow-artifacts

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# MLflow
MLFLOW_TRACKING_URI=http://localhost:5000

# Application
LOG_LEVEL=DEBUG
PROJECT_NAME=IRIS
